{
    "builds": [{
        "src": "/backend/wsgi.py",
        "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb", 
                "runtime": "python3.9"
            }
    },
    {
        "src": "build_files.sh",
        "use": "@vercel/static-build"
        
    }
],
    "routes": [
        {
            "src": "/(.*)",
            "destination": "/backend/wsgi.py"
        }
    ]
    
}
